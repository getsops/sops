{
    "comment": "Field path test suite. Backslash must be escaped in JSON strings.",

    "good": [
	["a",		"a"],
	["__x__",	"__x__"],
	["aBc0_d1",	"aBc0_d1"],
	["a.b",		"a", "b"],
	["`a`",		"a"],
	["`a`.b",	"a", "b"],
	["`a`.`b`.c",	"a", "b", "c"],
	["`a.b`.c",	"a.b", "c"],
	["`..`.`...`",	"..", "..."],
	["` `",		" "],
	["`\t\t`.` x\t`", "\t\t", " x\t"],
	["`\b\f\r`", "\b\f\r"],
	["`a\\`b`.`c\\\\d`",	"a`b", "c\\d"],
	["`\\\\`.`\\`\\``",	"\\", "``"]
    ],
    "bad": ["", " ", "\t", "a.", ".a", "a.b.", "a..b",
	"`", "``", "`a", "a`", "a`b", "`a`b", "a`b`", "`a`.b`c`",
	"\\", "\\`"]
}
