  <PageHeader as |p|>
    <p.top>
      <KmipBreadcrumb @showPath={{true}} />
    </p.top>
    <p.levelLeft>
      <h1 class="title is-3">
        <Icon
          @glyph="secrets"
          @size="xl"
          class="has-text-grey-light"
        />
        Configure KMIP secrets engine
      </h1>
    </p.levelLeft>
  </PageHeader>
  <EditForm
    @model={{model}}
    @onSave={{transition-to "vault.cluster.secrets.backend.kmip.configuration"}}
    @cancelLinkParams={{array "configuration"}}
  />
