<PageHeader as |p|>
  <p.top>
    <KmipBreadcrumb @scope={{this.scope}} @role={{this.role}}/>
  </p.top>
  <p.levelLeft>
    <h1 class="title is-3">
      Edit role
    </h1>
  </p.levelLeft>
</PageHeader>
<EditFormKmipRole
  @model={{model}}
  @onSave={{transition-to "vault.cluster.secrets.backend.kmip.role" this.scope this.role}}
  @cancelLinkParams={{array "role" this.scope this.role}}
/>
