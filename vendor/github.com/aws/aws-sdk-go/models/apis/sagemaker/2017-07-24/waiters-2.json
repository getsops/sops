    "TransformJobCompletedOrStopped": {
      "delay": 60,
      "maxAttempts": 60,
      "operation": "DescribeTransformJob",
      "acceptors": [
        {
          "expected": "Completed",
          "matcher": "path",
          "state": "success",
          "argument": "TransformJobStatus"
        },
        {
          "expected": "Stopped",
          "matcher": "path",
          "state": "success",
          "argument": "TransformJobStatus"
