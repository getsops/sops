        {
          "expected": "Stopped",
          "matcher": "path",
          "state": "success",
          "argument": "TrainingJobStatus"
        },
        {
          "expected": "Failed",
          "matcher": "path",
          "state": "failure",
          "argument": "TrainingJobStatus"
        },
        {
          "expected": "ValidationException",
          "matcher": "error",
          "state": "failure"
        }
      ]
    },
    "EndpointInService": {
      "delay": 30,
      "maxAttempts": 120,
      "operation": "DescribeEndpoint",
      "acceptors": [
        {
          "expected": "InService",
          "matcher": "path",
          "state": "success",
          "argument": "EndpointStatus"
