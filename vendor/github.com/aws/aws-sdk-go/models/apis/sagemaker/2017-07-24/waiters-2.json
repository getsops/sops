      "acceptors": [
        {
          "expected": "ValidationException",
          "matcher": "error",
          "state": "success"
        },
        {
          "expected": "Failed",
          "matcher": "path",
          "state": "failure",
          "argument": "EndpointStatus"
        }
      ]
